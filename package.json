{
  "name": "@dz-web/o-orange",
  "version": "3.7.2",
  "description": "A javascript utility library",
  "main": "./dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "utils",
    "util",
    "javascript",
    "frontend"
  ],
  "scripts": {
    "prepare": "dumi setup",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage && coveralls < coverage/lcov.info",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "build": "father build",
    "dev": "dumi dev",
    "docs:build": "cross-env APP_ROOT=docs dumi build",
    "docs:dep": "yarn docs:build && npx dz-ud /ld/o-orange",
    "docs:deploy": "yarn docs:build && git subtree push --prefix dist/ origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "moonlit",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "homepage": "https://moonlitusun.github.io/o-orange/",
  "devDependencies": {
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "dumi": "^2.1.19",
    "father": "^4.1.7",
    "increase-memory-limit": "^1.0.7",
    "jest": "^29.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "big.js": "^6.2.1"
  }
}
