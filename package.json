{
  "name": "o-orange",
  "version": "2.0.7",
  "description": "A javascript utility library",
  "main": "./index.js",
  "keywords": [
    "utils",
    "util",
    "javascript",
    "frontend"
  ],
  "scripts": {
    "clean": "rimraf lib/",
    "dist": "npm run clean && webpack --config ./dev/webpack.config.js",
    "example": "webpack --config ./dev/webpack.example.js",
    "start": "node ./dev/serve.js",
    "prepublishOnly": "npm run dist && npm run example",
    "pub": "npm publish --access public",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "ghpages": "npm run example && git subtree push --prefix example-dist/ origin gh-pages",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moonlitusun/o-orange"
  },
  "author": "moonlit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moonlitusun/o-orange/issues"
  },
  "files": [
    "lib",
    "index.js",
    "CHANGELOG.md"
  ],
  "homepage": "https://moonlitusun.github.io/o-orange/",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.3.0",
    "typescript": "^2.9.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^0.3.1"
  },
  "dependencies": {},
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/dev/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  }
}
