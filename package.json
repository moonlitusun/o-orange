{
  "name": "o-orange",
  "version": "2.1.3",
  "description": "A javascript utility library",
  "main": "./lib/o-orange.js",
  "keywords": [
    "utils",
    "util",
    "javascript",
    "frontend"
  ],
  "scripts": {
    "clean": "rimraf lib/",
    "build": "npm run clean && webpack --config ./dev/webpack.config.js",
    "start": "npm run clean && rollup -c",
    "prepublishOnly": "npm run build && npm run example",
    "pub": "npm publish --registry https://registry.npmjs.org/",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moonlitusun/o-orange"
  },
  "author": "moonlit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moonlitusun/o-orange/issues"
  },
  "files": [
    "lib",
    "index.js",
    "CHANGELOG.md"
  ],
  "homepage": "https://moonlitusun.github.io/o-orange/",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {}
}
