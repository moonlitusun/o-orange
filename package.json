{
  "name": "@dz-web/o-orange",
  "version": "3.5.11",
  "description": "A javascript utility library",
  "main": "./lib/orange.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "keywords": [
    "utils",
    "util",
    "javascript",
    "frontend"
  ],
  "scripts": {
    "clean": "rimraf lib/",
    "start": "npm run clean && rollup -c",
    "test": "jest --coverage && coveralls < coverage/lcov.info",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --notify",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "yarn docs:build && git subtree push --prefix dist/ origin gh-pages",
    "fix-memory-limit": "cross-env LIMIT=5000 increase-memory-limit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moonlitusun/o-orange"
  },
  "author": "moonlit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moonlitusun/o-orange/issues"
  },
  "files": [
    "lib",
    "es",
    "CHANGELOG.md"
  ],
  "homepage": "https://moonlitusun.github.io/o-orange/",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "core-js": "^3.7.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.7",
    "increase-memory-limit": "^1.0.7",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {}
}
