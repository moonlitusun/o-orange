{
  "name": "o-orange",
  "version": "0.0.2",
  "description": "a javascript utility library",
  "main": "./dist",
  "scripts": {
    "clean": "rimraf dist/",
    "dist": "npm run clean && webpack --config ./dev/webpack.config.js",
    "example": "webpack --config ./dev/webpack.example.js",
    "start": "node ./dev/serve.js",
    "prepublishOnly": "npm run dist && npm run example",
    "pub": "npm publish --access public",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "ghpages": "npm run example && git subtree push --prefix example-dist/ origin gh-pages",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "moonlit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moonlitusun/o-orange/issues"
  },
  "homepage": "https://github.com/moonlitusun/o-orange",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "raf": "^3.4.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.3.0",
    "typescript": "^2.9.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^0.3.1",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {},
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/dev/jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  }
}
